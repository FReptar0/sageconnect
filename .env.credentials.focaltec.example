# Configuración para Portal de Proveedores Focaltec
# Cambiar URL según ambiente (sandbox/producción)
URL=https://api-sandbox.portaldeproveedores.mx

# Configuración para múltiples tenants (separados por comas)
# TENANT_ID=tenant1,tenant2,tenant3
# API_KEY=key1,key2,key3
# API_SECRET=secret1,secret2,secret3
# DATABASES=DATABASE1,DATABASE2,DATABASE3
# EXTERNAL_IDS=RFC1,RFC2,RFC3

# Configuración para un solo tenant
TENANT_ID=your_tenant_id
API_KEY=your_api_key
API_SECRET=your_api_secret_token
DATABASES=YOUR_DATABASE_NAME
EXTERNAL_IDS=YOUR_RFC_ID